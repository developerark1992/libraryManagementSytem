@model IEnumerable<LMS.Models.Department>

@{
    ViewBag.Title = "listDepartment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>listDepartment</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeptID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeptName)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DeptID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeptName)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DeptID }) |
            @Html.ActionLink("Delete", "deleteCourse", new { id=item.DeptID })
        </td>
    </tr>
}

</table>
